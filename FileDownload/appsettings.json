{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",


  "ContentTypes": [
    {
      "Extension": ".jpg",
      "ContentType": "image/jpg"
    },
    {
      "Extension": ".png",
      "ContentType": "image/png"
    },
    {
      "Extension": ".jpeg",
      "ContentType": "image/jpeg"
    },
    {
      "Extension": ".pdf",
      "ContentType": "application/pdf"
    },
    {
      "Extension": ".exe",
      "ContentType": "application/octet-stream"
    },
    {
      "Extension": ".txt",
      "ContentType": "text/plain"
    }
  ]
}
